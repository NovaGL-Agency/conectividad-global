<style>
    .about
    {
        height: 100vh;
        width: 100%;
        display: flex;
        justify-content: space-between;
        align-items: center;
        padding: 100px;
        background: rgba(132, 147, 182, 0.2);
        gap: 60px;
    }

    .about-left
    {
        flex-basis: 40%;
        height: 100%;
        padding: 0 50px;
    }

    .about-left-back
    {
        width: 100%;
        height: 100%;
        background-color: #8493B6;
        border-radius: 20px;
        padding: 90px 70px 0 70px;
        position: relative;
    }

    .about-left-back-company
    {
        position: absolute;
        bottom: -20px;
        left: -50px;
        display: flex;
        align-items: center;
        gap: 20px;
        padding: 10px 20px;
        background-color: #FDF4EB;
        border-radius: 20px;
    }

    .about-left-container
    {
        width: 100%;
        height: 100%;
        border-radius: 20px 20px 0 0;
        background-color: #142133;
    }

    .about-right
    {
        flex-basis: 60%;
        display: flex;
        flex-direction: column;
        gap: 50px;
    }

    .about-right-title
    {
        display: flex;
        flex-direction: column;
        gap: 5px;
    }

    .about-right-title h4
    {
        color: #8493B6;
        font-size: 28px;
    }

    .about-right-title h2
    {
        font-size: 40px;
        font-weight: 700;
        color: #142133;
    }

    .about-right-benefits
    {
        display: flex;
        flex-direction: column;
        gap: 20px;
    }

    .about-right-benefits-item
    {
        display: flex;
        align-items: center;
        gap: 30px;
    }

    .about-right-benefits-item h5
    {
        font-size: 24px;
        font-weight: 700;
    }

    .about-right-benefits-item p
    {
        font-size: 16px;
        font-weight: 400;
        color: #818181;
    }

    .about-right-benefits-item-icon
    {
        position: relative;
        display: flex;
        align-items: center;
    }

    @media screen and (max-width: 1024px)
    {
        .about
        {
            flex-direction: column;
            padding: 50px 30px;
            height: 100vh;
        }

        .about-right h4
        {
            font-size: 24px;
        }

        .about-right h2
        {
            font-size: 30px;
        }

        .about-right-benefits
        {
            gap: 50px;
        }

        .about-right-benefits-item h5
        {
            font-size: 20px;
        }

        .about-right-benefits-item p
        {
            font-size: 12px;
        }

        .about-left
        {
            display: none;
        }
    }
</style>